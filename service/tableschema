CREATE TABLE user (
ID INTEGER PRIMARY KEY AUTOINCREMENT,
password TEXT NOT NULL,
 email TEXT NOT NULL,
 port INTEGER,
 ftpPort INTEGER,
 server CHAR(100),
 started DATE,
 running BOOLEAN,
 pid INTEGER
);

CREATE TABLE `registered_users` (
	`id`	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	`email`	TEXT NOT NULL
);
